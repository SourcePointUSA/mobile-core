                        "description": "Portal accountId",
                        "required": true,
                        "schema": {
                            "type": "integer",
                            "example": 22
                        }
                    },
                    {
                        "name": "propertyId",
                        "in": "query",
                        "description": "propertyId (found in portal)",
                        "required": true,
                        "schema": {
                            "type": "integer",
                            "example": 17801
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Metadata fetched sucessfully"
                    }
                }
            }
        }
    },
